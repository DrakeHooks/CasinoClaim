{
   "author": "Lavasoft",
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": {
         "19": "img/browsericons/icon19.png",
         "38": "img/browsericons/icon38.png"
      },
      "default_title": "Web Safety"
   },
   "content_scripts": [ {
      "all_frames": false,
      "css": [ "/css/contentscript.css" ],
      "js": [ "/js/contentscript.js" ],
      "matches": [ "https://*.yahoo.com/search*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "__MSG_extShortDesc__",
   "icons": {
      "128": "img/icon_128.png",
      "16": "img/icon_16.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "26.0",
   "name": "Web Safety",
   "permissions": [ "storage", "tabs", "unlimitedStorage", "webRequest", "webRequestBlocking", "http://*/*", "https://*/*" ],
   "short_name": "Web Safety",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.4",
   "web_accessible_resources": [ "img/*", "/loading-page.html" ]
}
